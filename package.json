{
  "name": "react-relay-14-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn run relay-compiler && DANGEROUSLY_DISABLE_HOST_CHECK=true react-scripts start",
    "build": "react-scripts build",
    "relay": "yarn run relay-compiler"
  },
  "dependencies": {
    "graphql": "^16.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "^14.1.0",
    "react-scripts": "5.0.1",
    "relay-runtime": "^14.1.0"
  },
  "devDependencies": {
    "@types/node": "18.11.10",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-relay": "14.1.2",
    "@types/relay-runtime": "14.1.5",
    "babel-plugin-relay": "^14.1.0",
    "relay-compiler": "^14.1.0",
    "nodemon": "2.0.20",
    "typescript": "4.9.3"
  },
  "proxy": "http://localhost:5000/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [],
  "description": "",
  "relay": {
    "src": "./src/",
    "schema": "./src/schema.graphql",
    "exclude": [
      "**/node_modules/**"
    ],
    "artifactDirectory": "src/__generated__",
    "language": "typescript"
  }
}
